{
    "initial:before": {
        "loopback#favicon": {}
    },
    "initial": {
        "compression": {
            "enabled": true,
            "params": {
                "threshold": 512
            }
        },
        "morgan": {
            "enabled": true,
            "params": [
                "dev"
            ]
        }
    },
    "session": {},
    "auth": {},
    "parse": {},
    "routes": {},
    "files": {},
    "final": {},
    "final:after": {
        "errorhandler": {
            "enabled": true,
            "params": {
                "dumpExceptions": true,
                "showStack": true
            }

        }


    }
}
